  "bugs": {
    "url": "https://github.com/SiddheshNan/Quiz-App-JS/issues"
  },
  "homepage": "https://github.com/SiddheshNan/Quiz-App-JS#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3"
  },
  "dependencies": {
    "sweetalert2": "^9.10.12"
  }
}
